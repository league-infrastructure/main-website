import Post from '../../../../../components/Post.astro';

const post = {
  title: "Carmel Valley youths win second place at robot competition",
  author: "ericbusboom",
  date: "2017-02-03",
  description: "Del Mar Times Newspaper, July 12, 2012",
  featuredImage: {
    src: "https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-scaled.jpeg",
    srcset: "https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-200x112.jpeg 200w, https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-400x225.jpeg 400w, https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-600x337.jpeg 600w, https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-800x450.jpeg 800w, https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-1200x675.jpeg 1200w, https://www.jointheleague.org/wp-content/uploads/2017/02/AdobeStock_187522353-scaled.jpeg 2560w",
