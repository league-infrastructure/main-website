---
import BaseLayout from '../../../../../layouts/BaseLayout.astro';
import Post from '../../../../../components/Post.astro';

const post = {
  title: "Former League Student Visits Classroom",
  author: "ericbusboom",
  date: "2017-02-03",
  description: "January 2017",
  featuredImage: {
    src: "https://www.jointheleague.org/wp-content/uploads/2017/02/Former-League-Student-Visits-Classroom.jpg",
    srcset: "https://www.jointheleague.org/wp-content/uploads/2017/02/Former-League-Student-Visits-Classroom-200x267.jpg 200w, https://www.jointheleague.org/wp-content/uploads/2017/02/Former-League-Student-Visits-Classroom-225x300.jpg 225w, https://www.jointheleague.org/wp-content/uploads/2017/02/Former-League-Student-Visits-Classroom.jpg 400w",
    sizes: "(max-width: 225px) 100vw, 225px",
  },
};
---
<BaseLayout title={post.title} description={post.description ?? undefined}>
  <Post title={post.title} author={post.author} date={post.date} featuredImage={post.featuredImage}>
    <p>January 2017</p>
  </Post>
</BaseLayout>
